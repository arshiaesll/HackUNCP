{
  "manifest_version": 3,
  "name": "Hack UNCP Extension",
  "description": "Base Level Extension",
  "version": "1.0",

  "action": {
    "default_popup": "index.html"
  },

  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["contentScript.js"]
    }
  ]
}
